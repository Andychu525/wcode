{
  "name": "monaco-web-ide",
  "version": "0.1.0",
  "description": "A rich code editor which runs inside a browser.",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fmsouza/monaco-web-ide.git"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "author": "Frederico Souza (fredericoamsouza@gmail.com)",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:start": "NODE_PATH=./src react-app-rewired start",
    "dev:server": "NODE_ENV=development node server .",
    "build": "NODE_PATH=./src react-app-rewired build",
    "prepublishOnly": "npm run build",
    "test": "NODE_PATH=./src react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "dependencies": {
    "express": "4.15.4",
    "keyboardjs": "2.3.3",
    "mime": "1.3.6",
    "mobx": "3.2.2",
    "mobx-react": "4.2.2",
    "opn": "5.1.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-monaco-editor": "0.10.0",
    "ws": "3.1.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.0.1",
    "npm-run-all": "4.0.2",
    "react-app-rewire-mobx": "1.0.6",
    "react-app-rewired": "1.2.0",
    "react-scripts": "1.0.11"
  }
}
