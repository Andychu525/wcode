{
  "name": "monaco-web-ide",
  "version": "0.1.0",
  "description": "A rich code editor which runs inside a browser.",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fmsouza/monaco-web-ide.git"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "author": "Frederico Souza (fredericoamsouza@gmail.com)",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "build": "rollup -c config/prod.js",
    "build:dev": "rollup -c config/dev.js -w",
    "browse": "browser-sync start --s --index public/index.dev.html --files build/**/*.js --no-notify",
    "server:dev": "NODE_ENV=development node server .",
    "start": "npm-run-all --parallel build:dev server:dev browse",
    "start:prod": "NODE_ENV=production node server .",
    "test": "npm run build:dev"
  },
  "dependencies": {
    "express": "4.15.4",
    "keyboardjs": "2.3.3",
    "mime": "1.3.6",
    "mobx": "3.2.2",
    "mobx-react": "4.2.2",
    "opn": "5.1.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-monaco-editor": "0.10.0",
    "ws": "3.1.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "browser-sync": "2.18.13",
    "npm-run-all": "4.0.2",
    "rollup": "0.48.2",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-postcss": "0.5.4",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "2.0.1"
  }
}
